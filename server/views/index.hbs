{{> header}}
{{> menu}}

<h2>Movies</h2>

<div class="cartas">
  {{#each movies}}
    <div class="carta">
      <h3>{{title}} ({{release_year}})</h3>
      <p><strong>Actors:</strong> {{actors}}</p>
    </div>
  {{/each}}
</div>

<h2>Categories</h2>

<ul>
  {{#each categories}}
    <li>{{name}}</li>
  {{/each}}
</ul>

{{> footer}}